{
    "name": "kassner/magento-behat-extension",
    "type": "magento-module",
    "description": "MageBehat - framework to run Behat/Mink for Magento extension integration tests",
    "homepage": "https://github.com/kassner/MageBehat",
    "license": "MIT",
    "keywords": ["behat","bdd","magento"],
    "require": {
        "magento-hackathon/magento-composer-installer": "*",
        "behat/behat": "2.4.4",
        "behat/mink-extension": "*",
        "behat/mink-browserkit-driver": "*",
        "behat/mink-selenium2-driver":"*",
        "behat/mink-goutte-driver": ">=1.0.3"
    },
    "autoload": {
        "psr-0": {
            "MageTest": "lib/MageTest/src"
        }
    },
    "minimum-stability": "dev"
}
